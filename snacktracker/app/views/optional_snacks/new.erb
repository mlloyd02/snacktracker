<%= form_for @snack do |f| %>
  <%= f.select :id, options_for_select(@not_suggested_snacks.map { |snack| [snack.name, snack.id] }), include_blank: true %>
  <%= f.submit "Suggest Snack" %>
<% end %>

<%= form_for @snack do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br />

  <%= f.label :location %>:
  <%= f.text_field :location %><br />

  <%= f.submit "Submit Suggestion" %>
<% end %>
